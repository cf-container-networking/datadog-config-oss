{
	"name": "app-sd add smoke tests failures (pws)",
	"type": "metric alert",
	"query": "min(last_4h):avg:cf_app_sd_smoke.ResolveAddSuccess{*} >= 0.5",
  "message": "smoke test failures indicate resolving an app via service discovery is not functioning @<%= alert_email %>",
	"tags": [],
		"timeout_h": 0,
		"notify_no_data": true,
		"no_data_timeframe": 120,
		"notify_audit": true,
		"require_full_window": true,
		"new_host_delay": 300,
		"include_tags": false,
		"escalation_message": "",
		"locked": false,
		"renotify_interval": "0",
		"evaluation_delay": "",
		"thresholds": {
			"critical": 0.5
		}
}
